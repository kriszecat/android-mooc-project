    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ResultsFragment_">
    <ListView
        style="@style/EventResultsListStyle"
        android:id="@+id/eventSummaryList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
